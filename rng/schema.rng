         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Ankerpunkt) Element bestimmt im Falle einer editorischen Anmerkung den Beginn der Stelle. [8.4.2. Synchronization and Overlap 16.5. Correspondence and Alignment]</a:documentation>
         <empty/>
         <attribute name="type">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <choice>
               <value>editor</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Beginn der Stelle</a:documentation>
            </choice>
         </attribute>
         <attribute name="xml:id">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <data type="string"/>
         </attribute>
         <empty/>
      </element>
   </define>
   <start>
      <choice>
         <ref name="undefinedTEI"/>
      </choice>
   </start>
   <s:schema xmlns:s="http://purl.oclc.org/dsdl/schematron"
             xmlns="http://www.tei-c.org/ns/1.0"
             xmlns:eg="http://www.tei-c.org/ns/Examples"
             xmlns:egXML="http://www.tei-c.org/ns/Examples"
             xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
                        <s:pattern id="duplicate_id">
                           <s:rule context="titleStmt">
                              <s:assert test="count(title[@type='main']) = 1">there should be only one main-title (Haupttitel), maybe use a sub-titel?</s:assert>
                           </s:rule>
                        </s:pattern>
                     </s:schema>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="tei_b_vg-address_usage-constraint-rule-29">
      <s:rule xmlns:s="http://purl.oclc.org/dsdl/schematron"
              xmlns="http://www.tei-c.org/ns/1.0"
              xmlns:eg="http://www.tei-c.org/ns/Examples"
              xmlns:egXML="http://www.tei-c.org/ns/Examples"
              xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
              context="//tei:address">
                        <s:report test="not(parent::tei:funder)">&lt;address&gt; schould be only
                           used in a funder element in the header!</s:report>
                     </s:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="tei_b_vg-country_settlement_usage-constraint-rule-30">
      <s:rule xmlns:s="http://purl.oclc.org/dsdl/schematron"
              xmlns="http://www.tei-c.org/ns/1.0"
              xmlns:eg="http://www.tei-c.org/ns/Examples"
              xmlns:egXML="http://www.tei-c.org/ns/Examples"
              xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
              context="//tei:country|//tei:settlement">
                        <s:report test="not(parent::tei:placeName)">&lt;country&gt; &amp;
                           &lt;settlement&gt; schould be only used in a funder element in the
                           header!</s:report>
                     </s:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="tei_b_vg-other_headerelements_usage-constraint-rule-31">
      <s:rule xmlns:s="http://purl.oclc.org/dsdl/schematron"
              xmlns="http://www.tei-c.org/ns/1.0"
              xmlns:eg="http://www.tei-c.org/ns/Examples"
              xmlns:egXML="http://www.tei-c.org/ns/Examples"
              xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
              context="//tei:placeName|//tei:persName|//tei:orgName|//tei:msDesc">
                        <s:let name="el_name" value="local-name(.)"/>
                        <s:report test="not(ancestor::tei:teiHeader)">&lt;"<s:value-of select="$el_name"/>"&gt; schould be only used the
                           teiHeader!</s:report>
                     </s:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="tei_b_vg-div_nesting-constraint-rule-32">
      <s:rule xmlns:s="http://purl.oclc.org/dsdl/schematron"
              xmlns="http://www.tei-c.org/ns/1.0"
              xmlns:eg="http://www.tei-c.org/ns/Examples"
              xmlns:egXML="http://www.tei-c.org/ns/Examples"
              xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
              context="//tei:div">
                        <s:report test="self::tei:div[@type='article']//tei:div[@type='section']">a
                           div representing an article shouldnât contain a div representing a
                           section</s:report>
                        <s:report test="self::tei:div[@type='article' and parent::*[@type!='section']]">a
                           div representing an article should allways have a div representing a
                           section as parent.</s:report>
                     </s:rule>
   </pattern>
</grammar>
