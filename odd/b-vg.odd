                     <s:rule context="//tei:address">
                        <s:report test="not(parent::tei:funder)">&lt;address&gt; schould be only used in a funder element in the header!</s:report>
                     </s:rule>
                  </constraint>
               </constraintSpec>
               <!-- stricter usage for country and settlement -->
               <constraintSpec ident="country_settlement_usage" scheme="schematron">
                  <constraint xmlns:s="http://purl.oclc.org/dsdl/schematron">
                     <s:rule context="//tei:country|//tei:settlement">
                        <s:report test="not(parent::tei:placeName)">&lt;country&gt; &amp; &lt;settlement&gt; schould be only used in a funder element in the header!</s:report>
