         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(personal name) contains a proper noun or proper-noun phrase referring to a person, possibly including one or more of the person's forenames, surnames, honorifics, added names, etc. [13.2.1. Personal Names]</a:documentation>
         <ref name="undefinedmacro.phraseSeq"/>
         <ref name="undefinedatt.global.attributes"/>
         <ref name="undefinedatt.datable.attributes"/>
         <ref name="undefinedatt.editLike.attributes"/>
         <ref name="undefinedatt.personal.attributes"/>
         <ref name="undefinedatt.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="undefinedplaceName">
      <element name="placeName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(place name) contains an absolute or relative place name. [13.2.3. Place Names]</a:documentation>
         <ref name="undefinedmacro.phraseSeq"/>
         <ref name="undefinedatt.datable.attributes"/>
         <ref name="undefinedatt.editLike.attributes"/>
         <ref name="undefinedatt.global.attributes"/>
         <ref name="undefinedatt.personal.attributes"/>
         <ref name="undefinedatt.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="undefinedcountry">
      <element name="country">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(country) contains the name of a geo-political unit, such as a nation, country, colony, or commonwealth, larger than or administratively superior to a region and smaller than a bloc. [13.2.3. Place Names]</a:documentation>
         <ref name="undefinedmacro.phraseSeq"/>
         <ref name="undefinedatt.global.attributes"/>
         <ref name="undefinedatt.naming.attributes"/>
         <ref name="undefinedatt.typed.attributes"/>
         <ref name="undefinedatt.datable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="undefinedsettlement">
      <element name="settlement">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(settlement) contains the name of a settlement such as a city, town, or village identified as a single geo-political or administrative unit. [13.2.3. Place Names]</a:documentation>
         <ref name="undefinedmacro.phraseSeq"/>
         <ref name="undefinedatt.global.attributes"/>
         <ref name="undefinedatt.naming.attributes"/>
         <ref name="undefinedatt.typed.attributes"/>
         <ref name="undefinedatt.datable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="undefinedatt.tableDecoration.attributes">
      <ref name="undefinedatt.tableDecoration.attribute.role"/>
      <ref name="undefinedatt.tableDecoration.attribute.rows"/>
      <ref name="undefinedatt.tableDecoration.attribute.cols"/>
   </define>
   <define name="undefinedatt.tableDecoration.attribute.role">
      <optional>
         <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                    name="role"
                    a:defaultValue="data">
            <a:documentation>(role) indicates the kind of information held in this cell or in each cell of this row.
Suggested values include: 1] label; 2] data</a:documentation>
            <choice>
               <value>label</value>
               <a:documentation>labelling or descriptive information only.</a:documentation>
               <value>data</value>
               <a:documentation>data values.</a:documentation>
               <data type="token">
                  <param name="pattern">[^\p{C}\p{Z}]+</param>
               </data>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="undefinedatt.tableDecoration.attribute.rows">
      <optional>
         <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                    name="rows"
                    a:defaultValue="1">
            <a:documentation>(rows) indicates the number of rows occupied by this cell or row.</a:documentation>
            <data type="nonNegativeInteger"/>
         </attribute>
      </optional>
   </define>
   <define name="undefinedatt.tableDecoration.attribute.cols">
      <optional>
         <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                    name="cols"
                    a:defaultValue="1">
            <a:documentation>(columns) indicates the number of columns occupied by this cell or row.</a:documentation>
            <data type="nonNegativeInteger"/>
         </attribute>
      </optional>
   </define>
   <define name="undefinedtable">
      <element name="table">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Tabelle) enthält Text, der in Tabellenform, also in Zeilen und Spalten, dargestellt ist. [14.1.1. TEI Tables]</a:documentation>
         <group>
            <zeroOrMore>
               <choice>
                  <ref name="undefinedmodel.headLike"/>
                  <ref name="undefinedmodel.global"/>
               </choice>
            </zeroOrMore>
            <choice>
               <oneOrMore>
                  <group>
                     <ref name="undefinedrow"/>
                     <zeroOrMore>
                        <ref name="undefinedmodel.global"/>
                     </zeroOrMore>
                  </group>
               </oneOrMore>
               <oneOrMore>
                  <group>
                     <ref name="undefinedmodel.graphicLike"/>
                     <zeroOrMore>
                        <ref name="undefinedmodel.global"/>
                     </zeroOrMore>
                  </group>
               </oneOrMore>
            </choice>
            <zeroOrMore>
               <group>
                  <ref name="undefinedmodel.divBottom"/>
                  <zeroOrMore>
                     <ref name="undefinedmodel.global"/>
                  </zeroOrMore>
               </group>
            </zeroOrMore>
         </group>
         <ref name="undefinedatt.global.attributes"/>
         <ref name="undefinedatt.typed.attributes"/>
         <optional>
            <attribute name="rows">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Zeilen) gibt die Anzahl der Tabellenzeilen an.</a:documentation>
               <data type="nonNegativeInteger"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="cols">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Spalten) gibt die Anzahl der Tabellenspalten an.</a:documentation>
               <data type="nonNegativeInteger"/>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="undefinedrow">
      <element name="row">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Tabellenzeile) enthält eine Zeile einer Tabelle. [14.1.1. TEI Tables]</a:documentation>
         <oneOrMore>
            <ref name="undefinedcell"/>
         </oneOrMore>
         <ref name="undefinedatt.global.attributes"/>
         <ref name="undefinedatt.tableDecoration.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="undefinedcell">
      <element name="cell">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Tabellenzelle) enthält eine Zelle einer Tabelle. [14.1.1. TEI Tables]</a:documentation>
         <ref name="undefinedmacro.specialPara"/>
         <ref name="undefinedatt.global.attributes"/>
         <ref name="undefinedatt.tableDecoration.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="undefinedatt.global.linking.attributes">
      <ref name="undefinedatt.global.linking.attribute.corresp"/>
      <ref name="undefinedatt.global.linking.attribute.synch"/>
      <ref name="undefinedatt.global.linking.attribute.sameAs"/>
      <ref name="undefinedatt.global.linking.attribute.copyOf"/>
      <ref name="undefinedatt.global.linking.attribute.next"/>
      <ref name="undefinedatt.global.linking.attribute.prev"/>
      <ref name="undefinedatt.global.linking.attribute.exclude"/>
      <ref name="undefinedatt.global.linking.attribute.select"/>
   </define>
   <define name="undefinedatt.global.linking.attribute.corresp">
      <optional>
         <attribute name="corresp">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(corresponds) points to elements that correspond to the current element in some way.</a:documentation>
            <list>
               <oneOrMore>
                  <data type="anyURI">
                     <param name="pattern">\S+</param>
                  </data>
               </oneOrMore>
            </list>
         </attribute>
      </optional>
   </define>
   <define name="undefinedatt.global.linking.attribute.synch">
      <optional>
         <attribute name="synch">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(synchronous) points to elements that are synchronous with the current element.</a:documentation>
            <list>
               <oneOrMore>
                  <data type="anyURI">
                     <param name="pattern">\S+</param>
                  </data>
               </oneOrMore>
            </list>
         </attribute>
      </optional>
   </define>
   <define name="undefinedatt.global.linking.attribute.sameAs">
      <optional>
         <attribute name="sameAs">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">points to an element that is the same as the current element.</a:documentation>
            <data type="anyURI">
               <param name="pattern">\S+</param>
            </data>
         </attribute>
      </optional>
   </define>
   <define name="undefinedatt.global.linking.attribute.copyOf">
      <optional>
         <attribute name="copyOf">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">points to an element of which the current element is a copy.</a:documentation>
            <data type="anyURI">
               <param name="pattern">\S+</param>
            </data>
         </attribute>
      </optional>
   </define>
   <define name="undefinedatt.global.linking.attribute.next">
      <optional>
         <attribute name="next">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">points to the next element of a virtual aggregate of which the current element is part.</a:documentation>
            <data type="anyURI">
               <param name="pattern">\S+</param>
            </data>
         </attribute>
      </optional>
   </define>
   <define name="undefinedatt.global.linking.attribute.prev">
      <optional>
         <attribute name="prev">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(previous) points to the previous element of a virtual aggregate of which the current element is part.</a:documentation>
            <data type="anyURI">
               <param name="pattern">\S+</param>
            </data>
         </attribute>
      </optional>
   </define>
   <define name="undefinedatt.global.linking.attribute.exclude">
      <optional>
         <attribute name="exclude">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">points to elements that are in exclusive alternation with the current element.</a:documentation>
            <list>
               <oneOrMore>
                  <data type="anyURI">
                     <param name="pattern">\S+</param>
                  </data>
               </oneOrMore>
            </list>
         </attribute>
      </optional>
   </define>
   <define name="undefinedatt.global.linking.attribute.select">
      <optional>
         <attribute name="select">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">selects one or more alternants; if one alternant is selected, the ambiguity or uncertainty is marked as resolved. If more than one alternant is selected, the degree of ambiguity or uncertainty is marked as reduced by the number of alternants not selected.</a:documentation>
            <list>
               <oneOrMore>
                  <data type="anyURI">
                     <param name="pattern">\S+</param>
                  </data>
               </oneOrMore>
            </list>
         </attribute>
      </optional>
   </define>
   <define name="undefinedseg">
      <element name="seg">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(arbiträres Segment) beschreibt Segmente eines Texts unterhalb des Chunk-Level. [16.3. Blocks, Segments, and Anchors 6.2. Components of the Verse Line 7.2.5. Speech Contents]</a:documentation>
         <ref name="undefinedmacro.paraContent"/>
         <ref name="undefinedatt.global.attribute.xmlid"/>
         <ref name="undefinedatt.global.attribute.n"/>
         <ref name="undefinedatt.global.attribute.xmllang"/>
         <ref name="undefinedatt.global.attribute.xmlbase"/>
         <ref name="undefinedatt.global.attribute.xmlspace"/>
         <attribute name="type">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">characterizes the element in some sense, using any convenient classification scheme or typology.</a:documentation>
            <choice>
               <value>note_ref</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Bezugsrahmen eines Kommentars</a:documentation>
            </choice>
         </attribute>
         <empty/>
      </element>
   </define>
   <start>
      <choice>
         <ref name="undefinedTEI"/>
      </choice>
   </start>
   <s:schema xmlns="http://www.tei-c.org/ns/1.0"
             xmlns:eg="http://www.tei-c.org/ns/Examples"
             xmlns:egXML="http://www.tei-c.org/ns/Examples"
             xmlns:s="http://purl.oclc.org/dsdl/schematron"
             xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
      <s:pattern id="duplicate_id">
         <s:rule context="titleStmt">
            <s:assert test="count(title[@type='main']) = 1">there should be only
                                 one main-title (Haupttitel), maybe use a sub-titel?</s:assert>
         </s:rule>
      </s:pattern>
   </s:schema>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="tei_b_vg-address_usage-constraint-rule-10">
      <s:rule xmlns="http://www.tei-c.org/ns/1.0"
              xmlns:eg="http://www.tei-c.org/ns/Examples"
              xmlns:egXML="http://www.tei-c.org/ns/Examples"
              xmlns:s="http://purl.oclc.org/dsdl/schematron"
              xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
              context="//tei:address">
         <s:report test="not(parent::tei:funder)">&lt;address&gt; schould be only
                           used in a funder element in the header!</s:report>
      </s:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="tei_b_vg-country_settlement_usage-constraint-rule-11">
      <s:rule xmlns="http://www.tei-c.org/ns/1.0"
              xmlns:eg="http://www.tei-c.org/ns/Examples"
              xmlns:egXML="http://www.tei-c.org/ns/Examples"
              xmlns:s="http://purl.oclc.org/dsdl/schematron"
              xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
              context="//tei:country|//tei:settlement">
         <s:report test="not(parent::tei:placeName)">&lt;country&gt; &amp;
                           &lt;settlement&gt; schould be only used in a funder element in the
                           header!</s:report>
      </s:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="tei_b_vg-other_headerelements_usage-constraint-rule-12">
      <s:rule xmlns="http://www.tei-c.org/ns/1.0"
              xmlns:eg="http://www.tei-c.org/ns/Examples"
              xmlns:egXML="http://www.tei-c.org/ns/Examples"
              xmlns:s="http://purl.oclc.org/dsdl/schematron"
              xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
              context="//tei:placeName|//tei:persName|//tei:orgName|//tei:msDesc">
         <s:let name="el_name" value="local-name(.)"/>
         <s:report test="not(ancestor::tei:teiHeader)">&lt;"<s:value-of select="$el_name"/>"&gt; schould be only used the
                           teiHeader!</s:report>
      </s:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="tei_b_vg-check_corr-constraint-rule-13">
      <s:rule xmlns="http://www.tei-c.org/ns/1.0"
              xmlns:eg="http://www.tei-c.org/ns/Examples"
              xmlns:egXML="http://www.tei-c.org/ns/Examples"
              xmlns:s="http://purl.oclc.org/dsdl/schematron"
              xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
              context="//tei:corr">
         <s:report test="parent::*[not(local-name()='choice')]"> a choice must be
                           placed inside "choice"-tag </s:report>
         <s:assert test="parent::*/tei:sic"> document the original variant with a
                           "sic"-tag </s:assert>
      </s:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="tei_b_vg-check_sic-constraint-rule-14">
      <s:rule xmlns="http://www.tei-c.org/ns/1.0"
              xmlns:eg="http://www.tei-c.org/ns/Examples"
              xmlns:egXML="http://www.tei-c.org/ns/Examples"
              xmlns:s="http://purl.oclc.org/dsdl/schematron"
              xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
              context="//tei:sic">
         <s:assert test="parent::*/tei:corr"> always use sic with corr </s:assert>
      </s:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="tei_b_vg-div_nesting-constraint-rule-15">
      <s:rule xmlns="http://www.tei-c.org/ns/1.0"
              xmlns:eg="http://www.tei-c.org/ns/Examples"
              xmlns:egXML="http://www.tei-c.org/ns/Examples"
              xmlns:s="http://purl.oclc.org/dsdl/schematron"
              xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
              context="//tei:div">
         <s:report test="self::tei:div[@type='article']//tei:div[@type='section']">a
                           div representing an article shouldn’t contain a div representing a
                           section</s:report>
         <s:report test="self::tei:div[@type='article' and parent::*[@type!='section']]">a
                           div representing an article should always have a div representing a
                           section as parent.</s:report>
         <s:report test="self::tei:div[@type='preamble' and parent::*[@type!='main']]">a div
                           representing a preamble should always be a direct child of the main
                           div</s:report>
      </s:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="tei_b_vg-head_rules-constraint-rule-16">
      <s:rule xmlns="http://www.tei-c.org/ns/1.0"
              xmlns:eg="http://www.tei-c.org/ns/Examples"
              xmlns:egXML="http://www.tei-c.org/ns/Examples"
              xmlns:s="http://purl.oclc.org/dsdl/schematron"
              xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
              context="//tei:head[@type='sub']">
         <s:assert test="parent::*/tei:head[@type='main']"> "head"-elements with
                           @type "sub" should have a sibling with @type "main" </s:assert>
      </s:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="tei_b_vg-check_note_position-constraint-rule-17">
      <s:rule xmlns="http://www.tei-c.org/ns/1.0"
              xmlns:eg="http://www.tei-c.org/ns/Examples"
              xmlns:egXML="http://www.tei-c.org/ns/Examples"
              xmlns:s="http://purl.oclc.org/dsdl/schematron"
              xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
              context="//tei:note[@type='editor']">
         <s:assert test="parent::tei:msItem"> note @type="editor" is only allowed
                           inside the ms-description of the document header </s:assert>
      </s:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="tei_b_vg-check_note_seg-constraint-rule-18">
      <s:rule xmlns="http://www.tei-c.org/ns/1.0"
              xmlns:eg="http://www.tei-c.org/ns/Examples"
              xmlns:egXML="http://www.tei-c.org/ns/Examples"
              xmlns:s="http://purl.oclc.org/dsdl/schematron"
              xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
              context="//tei:seg[@type='note_ref']">
         <s:assert test="./tei:note">a seg-Element of @type "note_ref" must contain a
                           notechild of @type "comment", that's its sole purpose </s:assert>
      </s:rule>
   </pattern>
</grammar>
